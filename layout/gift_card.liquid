<!doctype html>
<html class="no-js" lang="{{ shop.locale }}">
    <head>
    
    <script src="{{ 'vendor/qrcode.js' | shopify_asset_url }}" defer></script>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <meta name="theme-color" content="{{ settings.color_background }}">
    <link rel="canonical" href="{{ canonical_url }}">
    <link rel="preconnect" href="https://cdn.shopify.com" crossorigin>

    {%- if settings.favicon != blank -%}
      <link rel="icon" type="image/png" href="{{ settings.favicon | image_url: width: 32, height: 32 }}">
    {%- endif -%}

    {%- assign formatted_initial_value = gift_card.initial_value | money_without_trailing_zeros | strip_html -%}

    <title>{{ 'gift_cards.issued.title_html' | t: value: formatted_initial_value, shop: shop.name }}</title>

    <meta name="description" content="{{ 'gift_cards.issued.subtext' | t }}">

  <script src="https://www.googleoptimize.com/optimize.js?id=GTM-NC6XW4P"></script>
  <!-- OneTrust Cookies Consent Notice start -->
  <script src="https://cdn.cookielaw.org/scripttemplates/otSDKStub.js"  type="text/javascript" charset="UTF-8" data-domain-script="b096e828-f449-4e19-bcf1-d56c2826b500-test"></script>
  <script type="text/javascript">
    function OptanonWrapper(){}
  </script>
  <!-- OneTrust Cookies Consent Notice end -->
  {% include 'elevar-head' %}
  {% include 'elevar-custom' %}
  
       {% include 'style-tags', layout: 'theme' %}

    <!-- Product Meta ========================================= -->
    {% include 'open-graph-tags' %}
    {% include 'twitter-card' %}
  
    {% include 'script-tags', layout: 'theme' %}
    
    {{ content_for_header }}
    {{ 'assistance.css' | asset_url | stylesheet_tag }}
    
    <link href="{{ 'vendor.css' | asset_url }}" rel="stylesheet">
    {{"main.css" | asset_url | stylesheet_tag}}
    <script src="https://code.jquery.com/jquery-3.4.1.min.js" integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo=" crossorigin="anonymous"></script>
    
      <!-- Pollyfill.io ========================================= -->
    <script src="https://cdn.polyfill.io/v2/polyfill.min.js"></script>

    <!-- YOTPO ============================================= -->
    <script type="text/javascript">(function e(){var e=document.createElement("script");e.type="text/javascript",e.async=true,e.src="//staticw2.yotpo.com/7MWKpDOGOknXBXpo12TeaGWtIAs67lY9tGHxmig5/widget.js";var t=document.getElementsByTagName("script")[0];t.parentNode.insertBefore(e,t)})();</script>

    {% if template contains 'customers' %}
      {{ 'shopify_common.js' | shopify_asset_url | script_tag }}
    {% endif %}
    
      <!-- Klaviyo JS-->
    <script type="text/javascript" src="//www.klaviyo.com/media/js/public/klaviyo_subscribe.js"></script>

    {{ 'swiper-bundle.min.css' | asset_url | stylesheet_tag }}
    {{ 'swiper-bundle.min.js' | asset_url | script_tag }}
    {{ "lozad.js" | asset_url | script_tag}}
    {% include 'os20' %}
    {{ 'imgwithtext-phase2.css' | asset_url | stylesheet_tag }}
  
  {% render 'shogun-head' %}
</head>
  
<body id="{{ page_title | handle }}" class="eupopup eupopup-bottom template__{{ template | replace: '.', '--' }} {% if customer %}customer-logged-in{% endif %}" {% if template.name == '404' %}style="background-image: url({{ settings.image_404 | img_url: '1440x' }})" data-rjs="{{ settings.image_404 | img_url: '2880x' }}"{% endif %}>

    <div class="overlay" data-overlay></div>
     {%- include 'icon-store' -%}
 
    <header class="header" data-header>
      {%- section 'promo-bar' -%}
      {% section 'mega-menu' %}
    </header>
  
  <main class="main-content">
    {{ content_for_layout }}
  </main>
  
  {% include 'quiz' %}
  {%- include 'footer' -%}
  
   <script src="{{ 'vendor.js'  | asset_url }}"></script>
   {% include 'external-scripts' %}
  {% include 'elevar-body-end' %}
  {{ 'script.js' | asset_url | script_tag }}
  
  <style>
  .lozad{
    opacity: 0;
    transition: all 0.3s;
  }
  .fade {
  opacity: 1;
}
</style>
<script>
  lozad('.lozad', {
    load: function(el) {
        el.src = el.dataset.src;
        el.onload = function() {
            el.classList.add('fade')
        }
    }
}).observe()
</script>
